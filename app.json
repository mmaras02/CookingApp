{
        "expo": {
                "name": "Little chef",
                "slug": "little-chef",
                "version": "1.0.0",
                "orientation": "portrait",
                "icon": "./assets/images/icon3.png",
                "scheme": "myapp",
                "userInterfaceStyle": "automatic",
                "newArchEnabled": true,
                "updates": {
                        "url": "https://u.expo.dev/f954ee62-35b0-4abf-896e-fcf724b5baa7"
                },
                "runtimeVersion": {
                        "policy": "sdkVersion"
                },
                "splash": {
                        "image": "./assets/images/icon3.png",
                        "resizeMode": "contain",
                        "backgroundColor": "#558d77"
                },
                "ios": {
                        "supportsTablet": true,
                        "bundleIdentifier": "com.miamaras.CookingApp",
                        "infoPlist": {
                                "ITSAppUsesNonExemptEncryption": false
                        }
                },
                "android": {
                        "adaptiveIcon": {
                                "foregroundImage": "./assets/images/icon3.png",
                                "backgroundColor": "#558d77"
                        },
                        "permissions": ["android.permission.RECORD_AUDIO"],
                        "package": "com.miamaras.CookingApp",
                        "versionCode": 1
                },
                "web": {
                        "bundler": "metro",
                        "output": "static",
                        "favicon": "./assets/images/icon3.png"
                },
                "plugins": [
                        "expo-router",
                        [
                                "expo-splash-screen",
                                {
                                        "image": "./assets/images/icon3.png",
                                        "imageWidth": 200,
                                        "resizeMode": "contain",
                                        "backgroundColor": "#558d77"
                                }
                        ],
                        [
                                "expo-image-picker",
                                {
                                        "photosPermission": "Select and upload images.",
                                        "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
                                }
                        ]
                ],
                "experiments": {
                        "typedRoutes": true
                },
                "extra": {
                        "router": {
                                "origin": false
                        },
                        "eas": {
                                "projectId": "f954ee62-35b0-4abf-896e-fcf724b5baa7"
                        }
                }
        }
}
